<% exec = (1..50).step(2).to_a %>

{
  "title": {
    "text": "Runtime behavior of fold_right"
  },
  "series": [
    {
      "name": "hana::tuple",
      "data": <%= time_execution('execute.hana.tuple.erb.cpp', exec) %>
    }

    <%# Boost.Fusion >= 1.58.0 has a bug in reverse_fold %>
    <% if cmake_bool("@Boost_FOUND@") && "@Boost_VERSION@" != "105800" %>
    , {
      "name": "fusion::vector",
      "data": <%= time_execution('execute.fusion.vector.erb.cpp', exec) %>
    }
    <% end %>
  ]
}
